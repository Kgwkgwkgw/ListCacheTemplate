<?xml version="1.0" encoding="UTF-8"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="http://www.ehcache.org/v3"
        xsi:schemaLocation="http://www.ehcache.org/v3
                            http://www.ehcache.org/schema/ehcache-core-3.0.xsd">

    <cache alias="stringSingleKeyExampleDto">
        <key-type>java.lang.String</key-type>
        <value-type>org.tommy.dto.StringSingleKeyExampleDto</value-type>
        <expiry>
            <ttl unit="seconds">600</ttl>
        </expiry>
        <resources>
            <heap unit="entries">60000</heap>
        </resources>
    </cache>

    <cache alias="longSingleKeyExampleDto">
        <key-type>java.lang.String</key-type>
        <value-type>org.tommy.dto.LongSingleKeyExampleDto</value-type>
        <expiry>
            <ttl unit="seconds">600</ttl>
        </expiry>
        <resources>
            <heap unit="entries">60000</heap>
        </resources>
    </cache>

    <cache alias="compositeKeyExampleDto">
        <key-type>java.lang.String</key-type>
        <value-type>org.tommy.dto.CompositeKeyExampleDto</value-type>
        <expiry>
            <ttl unit="seconds">600</ttl>
        </expiry>
        <resources>
            <heap unit="entries">60000</heap>
        </resources>
    </cache>
</config>
